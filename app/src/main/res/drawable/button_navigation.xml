<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="@dimen/_10sdp" />
            <stroke android:width="@dimen/standard_button_border_width" android:color="#FFFFFF" />
            <gradient android:angle="-90" android:startColor="#345953" android:endColor="#689a92"  />
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/_10sdp" />
            <stroke android:width="@dimen/standard_button_border_width" />
            <gradient android:angle="-90" android:startColor="@color/button_next_inner" android:endColor="#888888"  />
        </shape>
    </item>
</selector>
