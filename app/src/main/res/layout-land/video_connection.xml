<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.widget.Button
        android:id="@+id/button_url_settings"
        style="@style/button_neutral"
        android:layout_width="@dimen/standard_button_width_land"
        android:layout_marginTop="@dimen/margin_top_land"
        android:text="@string/url_settings"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <android.widget.Button
        android:id="@+id/button_invite_partner"
        style="@style/button_neutral"
        android:layout_width="@dimen/standard_button_width_land"
        android:layout_height="@dimen/standard_button_height_land"
        android:layout_marginTop="@dimen/margin_horizontal_big_land"
        android:text="@string/invite_partner"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_url_settings" />

    <android.widget.Button
        android:id="@+id/button_access_videocall"
        style="@style/button_next"
        android:layout_width="0dp"
        android:layout_height="@dimen/standard_button_height_land"
        android:layout_marginStart="@dimen/margin_horizontal_small_land"
        android:layout_marginEnd="@dimen/margin_bottom_land"
        android:layout_marginBottom="@dimen/margin_bottom_land"
        android:text="@string/access_videocall"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button_test_controls" />

    <android.widget.Button
        android:id="@+id/button_test_controls"
        style="@style/button_back"
        android:layout_width="0dp"
        android:layout_marginStart="@dimen/margin_side_land"
        android:layout_marginEnd="@dimen/margin_horizontal_small_land"
        android:layout_marginBottom="@dimen/margin_bottom_land"
        android:text="@string/test_controls"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button_access_videocall"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/text_meeting_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/margin_side_land"
        android:layout_marginBottom="@dimen/margin_vertical_big_land"
        app:layout_constraintBottom_toTopOf="@+id/button_test_controls"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>